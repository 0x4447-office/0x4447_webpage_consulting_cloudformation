{
	"CalendarLambda": {
		"Type": "AWS::Lambda::Function",
		"Properties": {
			"FunctionName": "calendar",
			"Code": {
				"ZipFile": { "Fn::Join": ["", [
					"exports.handler = async (event) => {return true;};"
				]]}
			},
			"Handler": "index.handler",
			"MemorySize": 128,
			"Role": { "Fn::GetAtt": ["CalendarLambdaRole", "Arn"] },
			"Runtime": "nodejs12.x",
			"Timeout": 60,
			"Environment": {
				"Variables": {
					"STAGE": { "Ref": "StageParam" }
				}
			}
		}
	}
}
